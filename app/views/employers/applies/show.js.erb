<% if @apply.present? %>
  $('#area-form-status').html('<%= j render partial: "employers/applies/form_status", locals: {apply: @apply, current_step: @current_step, current_status_steps: @current_status_steps, company_stattus_step_ids: @company_stattus_step_ids} %>');
  $('#area-hisory-apply').html('<%= j render partial: "employers/apply_statuses/apply_status", collection: @apply_statuses, as: :apply_status %>');
  $('#pagination-history-apply').html('<%= j paginate @apply_statuses %>');
<% end %>
